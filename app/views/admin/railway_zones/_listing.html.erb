<tr id=<%=railway_zone.id.to_s%>>
  <td class="text-center"><span class="view-location-<%= index %>"><%= railway_zone.code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= railway_zone.name rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= railway_zone.zone_headquarter rescue '' %></span></td>
  
  <td class="text-center"><span class="view-location-<%= index %>"><%= railway_zone.divisions.pluck(:code).join(", ") rescue '' %></span></td>
  
  <td class="text-center" style="white-space: nowrap">
    <span class="view-location-<%= index %>">
      <%= link_to '<i class="fa fa-pencil"></i>'.html_safe, edit_admin_railway_zone_path(railway_zone), :remote => true, class: "label label-default", title: "Edit RailwayZone" %>
      
     

      <%= link_to "<i class='fa fa-trash-o'></i>".html_safe, "javascript:;", class: 'label label-danger railway_zone-delete', data: {toggle: "modal", method_type: "delete", link: admin_railway_zone_path(railway_zone.id), message: "You really want to delete this RailwayZone?", target: "#custom-confirm-box"}, remote: true, title: "Delete RailwayZone" %>
    </span>
  </td>
</tr>
     