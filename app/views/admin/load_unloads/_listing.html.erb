<tr id=<%=load_unload.id.to_s%>>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.station.name rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.serving_station rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.station.code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.status rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.division.code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.area.area_code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= load_unload.desc rescue '' %></span></td>
  
  <%if is_viewable%>
    <td class="text-center" style="white-space: nowrap">
      <span class="view-location-<%= index %>">
        <%= link_to '<i class="fa fa-pencil"></i>'.html_safe, edit_admin_load_unload_path(load_unload), :remote => true, class: "label label-default", title: "Edit Loading and Unloading point" %>
        
       

        <%= link_to "<i class='fa fa-trash-o'></i>".html_safe, "javascript:;", class: 'label label-danger area-delete', data: {toggle: "modal", method_type: "delete", link: admin_load_unload_path(load_unload.id), message: "You really want to delete this Loading/Unloading point?", target: "#custom-confirm-box"}, remote: true, title: "Delete oading and Unloading point" %>
      </span>
    </td>
  <%end%>
</tr>
     