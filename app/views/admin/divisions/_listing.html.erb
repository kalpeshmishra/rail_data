<tr id=<%=division.id.to_s%>>
  <td class="text-center"><span class="view-location-<%= index %>"><%= division.code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= division.name rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= division.railway_zone.code rescue '' %></span></td>
  
  <%if is_viewable%>
	  <td class="text-center" style="white-space: nowrap">
	    <span class="view-location-<%= index %>">
	      <%= link_to '<i class="fa fa-pencil"></i>'.html_safe, edit_admin_division_path(division), :remote => true, class: "label label-default", title: "Edit Division" %>
	      
	     

	      <%= link_to "<i class='fa fa-trash-o'></i>".html_safe, "javascript:;", class: 'label label-danger division-delete', data: {toggle: "modal", method_type: "delete", link: admin_division_path(division.id), message: "You really want to delete this Division?", target: "#custom-confirm-box"}, remote: true, title: "Delete Division" %>
	    </span>
	  </td>
	<%end%>
</tr>
     