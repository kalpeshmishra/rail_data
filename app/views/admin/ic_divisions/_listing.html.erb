<tr id=<%=ic_division.id.to_s%>>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.railway_zone.code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.division.code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.from_section rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.ic_code rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.ic_name rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.to_section rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.to_division rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.to_zone rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.ic_by rescue '' %></span></td>
  <td class="text-center"><span class="view-location-<%= index %>"><%= ic_division.interchange_type rescue '' %></span></td>
  <%if is_viewable%>  
    <td class="text-center" style="white-space: nowrap">
      <span class="view-location-<%= index %>">
        <%= link_to '<i class="fa fa-pencil"></i>'.html_safe, edit_admin_ic_division_path(ic_division), :remote => true, class: "label label-default", title: "Edit IcDivision" %>
       
        <%= link_to "<i class='fa fa-trash-o'></i>".html_safe, "javascript:;", class: 'label label-danger ic_division-delete', data: {toggle: "modal", method_type: "delete", link: admin_ic_division_path(ic_division.id), message: "You really want to delete this IcDivision?", target: "#custom-confirm-box"}, remote: true, title: "Delete IcDivision" %>
  	  </span>
    </td>
  <%end%>
</tr>
     